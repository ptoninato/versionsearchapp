@model VersionSearchViewModel


@using (Html.BeginForm("SearchResults", "Home", FormMethod.Get))
{
    <div>Enter Version Number:</div>
    <div>
        @Html.TextBoxFor(m => m.SearchTerm, new { placeholder = "0.0.0.0" })
        @Html.ValidationMessageFor(m => m.SearchTerm, "")
    </div>
    <br />
    <input type="submit" class="btn btn-primary" value="Search" />
}
